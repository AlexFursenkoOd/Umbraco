@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Home>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
}
<section class='col-sm-6'>@Umbraco.Field("description", altFieldAlias:"aboutUs")</section>
<img class='col-sm-4' src='@Umbraco.Media(Int32.Parse(Umbraco.Field("image").ToString())).Url'/>